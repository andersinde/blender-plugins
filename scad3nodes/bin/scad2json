#!/bin/sh
file=$1
# file=${file%.*}
# file=${file##*/}

echo "{$file}"
cp $1 /tmp/tmp.scad

OpenSCAD /tmp/tmp.scad -o /tmp/tmp.csg
# cat /tmp/tmp.csg

parse < /tmp/tmp.csg > /tmp/nodes.py
touch /tmp/${file}.name
 
# cat /tmp/nodes.py

rm /tmp/tmp.scad /tmp/tmp.csg
echo "/tmp/nodes.py" > /tmp/blender_tmp.io
